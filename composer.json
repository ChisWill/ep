{
    "name": "chiswill/ep",
    "description": "Elastic PHP Framework",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "httpsoft/http-message": "^1.0.5",
        "yiisoft/di": "^3.0",
        "yiisoft/log": "^3.0",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/db-mysql": "^3.0",
        "yiisoft/active-record": "^3.0",
        "yiisoft/validator": "^3.0"
    },
    "autoload": {
        "files": [
            "src\\Ep.php",
            "src\\Base\\function.php"
        ],
        "psr-4": {
            "Ep\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ep\\Tests\\App\\": "tests\\app\\src",
            "Ep\\Tests\\Cls\\": "tests\\cls"
        }
    }
}