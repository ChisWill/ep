{
    "name": "chiswill/ep",
    "description": "Elastic PHP Framework",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "httpsoft/http-message": "^1.0.5",
        "nikic/fast-route": "^1.3",
        "yiisoft/active-record": "^3.0",
        "yiisoft/arrays": "^1.0",
        "yiisoft/cache-file": "dev-master",
        "yiisoft/db-mysql": "^3.0",
        "yiisoft/db-redis": "1.0.x-dev",
        "yiisoft/di": "^3.0",
        "yiisoft/http": "^1.0",
        "yiisoft/log": "^3.0",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/router-fastroute": "3.0.x-dev@dev",
        "yiisoft/strings": "^1.0",
        "yiisoft/validator": "^3.0",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "4.x-dev"
    },
    "autoload": {
        "files": [
            "src\\Ep.php",
            "src\\Base\\functions.php"
        ],
        "psr-4": {
            "Ep\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ep\\Tests\\App\\": "tests\\app\\src",
            "Ep\\Tests\\Classes\\": "tests\\classes"
        }
    },
    "config": {
        "sort-packages": true
    }
}
