{
    "name": "chiswill/ep",
    "description": "Elastic PHP Framework",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "httpsoft/http-message": "^1.0.5",
        "nikic/fast-route": "^1.3",
        "yiisoft/active-record": "dev-master",
        "yiisoft/arrays": "^1.0",
        "yiisoft/cache-file": "dev-master",
        "yiisoft/db-mysql": "dev-master",
        "yiisoft/db-redis": "1.0.x-dev",
        "yiisoft/di": "^3.0",
        "yiisoft/http": "^1.0",
        "yiisoft/log": "^1.0",
        "yiisoft/log-target-file": "dev-master",
        "yiisoft/router-fastroute": "3.0.x-dev@dev",
        "yiisoft/session": "dev-master",
        "yiisoft/strings": "^2.0",
        "yiisoft/validator": "^3.0",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.0.0",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.5"
    },
    "autoload": {
        "files": [
            "src\\Ep.php",
            "src\\Base\\functions.php"
        ],
        "psr-4": {
            "Ep\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ep\\Tests\\": "tests",
            "Ep\\Tests\\Basic\\": "tests/AppBasic/src",
            "Ep\\Tests\\Advance\\": "tests/AppAdvance/src",
            "Ep\\Tests\\Support\\": "tests/Support"
        }
    },
    "config": {
        "sort-packages": true
    }
}